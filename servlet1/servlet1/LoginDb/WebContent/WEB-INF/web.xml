<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>LoginDb</display-name>
	<servlet>
		<description>
		</description>
		<display-name>CheckAccess</display-name>
		<servlet-name>CheckAccess</servlet-name>
		<servlet-class>CheckAccess</servlet-class>
		<init-param>
		  <param-name>driverclassname</param-name>
		  <param-value>com.mysql.jdbc.Driver</param-value>
	        </init-param>

		<init-param>
		  <param-name>dburl</param-name>
		  <param-value>jdbc:mysql://localhost/namaDB</param-value>
	        </init-param>

		<init-param>
		  <param-name>username</param-name>
		  <param-value>root</param-value>
	        </init-param>

		<init-param>
		  <param-name>password</param-name>
		  <param-value>namrata</param-value>
	        </init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>CheckAccess</servlet-name>
		<url-pattern>/Check</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		
	</welcome-file-list>
</web-app>
